{
  "name": "LinkedIn Job Hider",
  "description": "Chrome extension designed to remove \"We won't show you this job again.\"",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [ "https://www.linkedin.com/jobs/*" ],
      "js": [ "scripts/test.js" ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "assets/icons/icon.png"
  }
}